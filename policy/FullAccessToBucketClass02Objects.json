{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "FullAccessToBucketClass02Objects",
        "Effect": "Allow",
        "Action": [
          "s3:ListBucket",
          "s3:GetObject",
          "s3:PutObject",
          "s3:DeleteObject"
        ],
        "Resource": [
          "arn:aws:s3:::class02",
          "arn:aws:s3:::class02/*"
        ]
      }
    ]
  }
  